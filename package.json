{
  "name": "photo_app",
  "productName": "Photo App",
  "version": "1.0.0",
  "description": "A photo management application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && vite build",
    "build:main": "tsc",
    "build:renderer": "vite build",
    "start": "npm run build && electron dist/index.js",
    "dev": "concurrently \"tsc -w\" \"vite\" \"wait-on dist/index.js && wait-on http://localhost:5173 && electron dist/index.js\"",
    "dev:vite": "vite",
    "clean": "rm -rf dist"
  },
  "author": {
    "name": "Scott Barstow",
    "email": "sbarstow@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "chokidar": "^4.0.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^8.2.0",
    "exifr": "^7.1.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-window": "^1.8.10",
    "sharp": "^0.33.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.2.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "electron": "^37.2.5",
    "electron-rebuild": "^3.2.9",
    "postcss": "^8.4.33",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.9.2",
    "vite": "^5.0.12",
    "wait-on": "^8.0.0"
  }
}
